{
  "name": "brilo-dev-stack",
  "version": "1.0.0",
  "description": "Gulp for front-end development",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "targets": {
    "frontend": {
      "source": "./src/js/index.ts",
      "distDir": "./dist/"
    }
  },
  "scripts": {
    "dev:gulp": "gulp",
    "dev:parcel": "parcel watch --no-hmr",
    "dev": "npm run clean && npm run build:gulp && concurrently --raw npm:lint:* && concurrently --raw --kill-others npm:dev:* npm:watch:lint:*",
    "devSilent": "npm run dev --silent",
    "build:parcel": "parcel build",
    "build:gulp": "gulp build",
    "build": "npm run clean && npm run deploy && npm run copy",
    "deploy": "npm run build:gulp && npm run build:parcel && concurrently --raw npm:lint:*",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint:js": "eslint .",
    "watch:lint:scss": "chokidar \"src/**/*.scss\" -c \"npm run lint:scss\" --silent",
    "watch:lint:js": "chokidar \"src/**/*.{ts,js,tsx}\" -c \"npm run lint:js\" --silent",
    "serve": "npx serve dist",
    "copy": "gulp copy",
    "clean": "rimraf dist && rimraf .parcel-cache && mkdirp -p ./dist/images/"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@parcel/config-default": "^2.10.3",
    "@parcel/transformer-sass": "^2.10.3",
    "@parcel/transformer-typescript-tsc": "^2.10.3",
    "@parcel/validator-typescript": "^2.10.3",
    "@types/headroom.js": "^0.12.4",
    "@types/smoothscroll-polyfill": "^0.3.3",
    "@types/vanillajs-datepicker": "^1.3.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "autoprefixer": "^10.4.16",
    "browser-sync": "^2.29.3",
    "chokidar": "^3.5.3",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^8.2.2",
    "cssnano": "^6.0.2",
    "del": "^6.1.1",
    "eslint": "^8.55.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "gulp": "^4.0.2",
    "gulp-data": "^1.3.1",
    "gulp-format-html": "^2.3.0",
    "gulp-newer": "^1.4.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sass-glob": "^1.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-svgmin": "^4.1.0",
    "gulp-twig": "^1.2.0",
    "mkdirp": "^3.0.1",
    "parcel": "^2.10.3",
    "postcss": "^8.4.32",
    "postcss-100vh-fix": "^1.0.2",
    "postcss-em-media-query": "^4.1.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-pxtorem": "^6.0.0",
    "postcss-viewport-height-correction": "^1.1.1",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "sass": "~1.64.2",
    "stylelint": "^16.0.2",
    "stylelint-config-standard-scss": "^12.0.0",
    "stylelint-prettier": "^5.0.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.10",
    "@fullcalendar/daygrid": "^6.1.10",
    "@fullcalendar/interaction": "^6.1.10",
    "bootstrap": "^5.3.2",
    "flatpickr": "^4.6.13",
    "hc-offcanvas-nav": "^6.1.5",
    "headroom.js": "^0.12.0",
    "highlight.js": "^11.9.0",
    "js-beautify": "^1.14.11",
    "smoothscroll-polyfill": "^0.4.4",
    "uikit": "^3.17.11",
    "validator": "^13.11.0",
    "vanilla-lazyload": "^17.8.5"
  },
  "overrides": {
    "svgo": "^3.0.2",
    "chokidar": "^3.5.3",
    "glob-stream": "^7.0.0",
    "glob-parent": "^6.0.2",
    "micromatch": "^4.0.5",
    "twig": "^1.16.0"
  }
}
